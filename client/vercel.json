{
    "functions": {
        "api/*.js": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)", 
            "destination": "https://trail-finder-washington-server.vercel.app/api/$1"
        },
        {
            "source": "/(.*)", 
            "destination": "/index.html"
        }
    ]
}
